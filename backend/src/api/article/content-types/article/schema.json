{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "articleCategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-category.article-category",
      "inversedBy": "articles"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.grid",
        "content.rich-content"
      ],
      "required": false
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
